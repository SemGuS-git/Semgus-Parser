{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.sln", "*/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml", "restapi/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "SemGuS Parser Documentation",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
